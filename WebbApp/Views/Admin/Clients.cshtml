@model ClientsViewModel
@{
	ViewData["Title"] = "Clients";
}

@await Html.PartialAsync("Partials/Sections/_AddClientModal", Model.AddClientForm)
@await Html.PartialAsync("Partials/Sections/_EditClientModal", Model.EditClientForm)



<section id="clients">

	<header class="portal-layout-main-header">
		<h2>Clients</h2>
		<button type="button" data-modal="true" data-target="#addClientModal" class="btn btn-blue btn-add"><i class="fa-solid fa-plus"></i>Add Client</button>
	</header>

	@await Html.PartialAsync("Partials/Sections/_ClientList", Model.Clients)


</section>

